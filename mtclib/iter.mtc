external int strlen(string s)

array range(int integer){
    array arr = []
    if (integer <= 0){
        return arr
    }
    int rnge = 0
    while (rnge < integer){
        arr.append(rnge)
        set rnge = rnge+1
    }
    return arr
}
array strarray(string text){
    array items = []
    int i = 0
    int len = strlen(text)
    while (i < len) {
        items.append(text[i])
        set i = i + 1
    }
    return items
}